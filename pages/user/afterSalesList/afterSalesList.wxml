<!-- afterSalesList.wxml -->
<view class="user">
  <view class="about-me flex p30 mb-20">
    <view class="member-photo">
        <image src="{{ userInfo.avatarUrl }}"></image>
    </view>
    <view class="member-info">
        <view>{{ userInfo.nickName }}</view>
    </view>
  </view>
  <view class="menu-list">
    <view class="list-item">
      <navigator class="p30" url="../user/order/order">
        <text class="menu-name">我的订单</text>
        <view class="arrow pr"></view>
      </navigator>
    </view>
    <view class="list-item">
      <navigator class="p30" url="../user/address/address">
        <text class="menu-name">我的地址</text>
        <view class="arrow pr"></view>
      </navigator>
    </view>
  </view>
</view>
